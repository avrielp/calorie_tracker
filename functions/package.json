    "firebase-functions": "^6.4.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "typescript": "^5.8.3"
  }
}


